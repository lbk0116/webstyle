<openerp>
    <data>
        <template id="idname" name="stock assets" inherit_id="web.assets_common">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/webstyle/static/src/css/customStyle.css" />
                <script type="text/javascript" src="/webstyle/static/src/js/customScript.js"></script>
            </xpath>
        </template>
        <!--快捷链接-->
        <template id="shortcut" name="stock assets" inherit_id="web.webclient_bootstrap">
            <xpath expr="." position="inside">
                <div class="shortcut">
                    <div class='item'></div>
                    <div class='toggleBtn'></div>
                </div>
                <script type="text/javascript" src="/webstyle/static/src/js/shortcutLink.js"></script>
            </xpath>
        </template>
     </data>
</openerp>